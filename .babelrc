{
  "presets": ["es2015"],
  "plugins": [
    "transform-class-properties",
    "transform-react-jsx",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "test": "./test",
        "store": "./src/store"
      }
      }],
      ["import-rename", {"^(.*)\\.jsx$": "$1"}]
    ]
  }
